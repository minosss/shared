{
	"name": "@yme/react-shared",
	"description": "collection of utilities for react",
	"version": "0.6.0",
	"type": "module",
	"author": "mino",
	"repository": "minosss/shared",
	"license": "MIT",
	"exports": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src --dts",
		"build:fast": "tsup src",
		"dev": "pnpm build:fast -- --watch",
		"clean": "rimraf dist .turbo",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@yme/shared": "workspace:*"
	},
	"devDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"engines": {
		"node": "^14.14.0 || >=16.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"react": "^18",
		"react-dom": "^18"
	},
	"tsup": {
		"clean": true,
		"target": "es2020",
		"format": [
			"esm"
		]
	}
}
