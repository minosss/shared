{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"build:fast": {
			"outputs": ["dist/**"],
			"dependsOn": ["^build:fast"]
		},
		"dev": {
			"cache": false
		},
		"typecheck": {
			"cache": false,
			"dependsOn": ["^typecheck"]
		},
		"test": {}
	}
}
